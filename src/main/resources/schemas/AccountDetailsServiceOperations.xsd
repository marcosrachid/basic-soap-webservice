<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns="http://com/soap/template/webservices/accountservice"
	xmlns:account="http://webservices.template.soap.com" targetNamespace="http://com/soap/template/webservices/accountservice"
	elementFormDefault="qualified">
	<xsd:import namespace="http://webservices.template.soap.com"
		schemaLocation="AccountDetails.xsd" />
	<xsd:element name="AccountDetailsRequest">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="accountNumber" type="xsd:string" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="AccountDetailsResponse">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="AccountDetails" type="account:Account" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>  